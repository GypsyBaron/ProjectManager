<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CompanyController">
   <children>
      <VBox prefHeight="420" prefWidth="600.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
         <children>
            <HBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="21.0" prefWidth="390.0" text="Company Login" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="20.0" right="30.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="companyLogin" prefHeight="31.0" prefWidth="356.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" top="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="21.0" prefWidth="300.0" text="Company password" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="companyPass" prefHeight="31.0" prefWidth="254.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" top="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="21.0" prefWidth="340.0" text="Company Title" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="20.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="companyTitle" prefHeight="31.0" prefWidth="295.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#changeCompanyInfo" text="Save changes" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
         <HBox.margin>
            <Insets top="20.0" />
         </HBox.margin>
      </VBox>
   </children>
</HBox>
